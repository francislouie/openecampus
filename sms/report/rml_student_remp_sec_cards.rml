					<para style="P2">[[ (s['pic2']) and setTag('para','image',{'width':'2.2cm','height':'2.6cm'}) ]][[ s['pic2'] ]]</para>
				        </td>
						</tr>
				    </blockTable>

				    <blockTable colWidths="9cm" style="subims">
						<tr>
				        	<td>		
							<para style="frontims"><b>Forward Girls College, Peshawar</b></para>
				        	</td>
						</tr>
				    </blockTable>
				    
					<para style="space">
					<font color="white"> </font>
					</para>
		        </td>
				</tr>
		    </blockTable>
    	</td>
    	</tr>
    </blockTable>

</section>




</story>
</document>
