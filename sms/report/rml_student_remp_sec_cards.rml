<?xml version="1.0"?>
<document filename="test.pdf">
  <template pageSize="(21cm, 29.7cm)" title="Test" author="Mohsin Yaseen" >
    <pageTemplate id="first">
      <frame id="first" x1="18" y1="10" width="21cm" height="29.7cm"/>
    </pageTemplate>
  </template>
  <stylesheet>
    <blockTableStyle id="Standard_Outline">
      <blockValign value="TOP"/>
      <blockAlignment value="CENTER"/>
    </blockTableStyle>
    
    <blockTableStyle id="TableGrid">
      <blockAlignment value="LEFT"/>
      <blockValign value="TOP"/>

    </blockTableStyle>

    <blockTableStyle id="subims">
	<blockBackground colorName="#0033CC" />
    </blockTableStyle>

    <initialize>
      <paraStyle name="all" alignment="justify"/>
    </initialize>
    <paraStyle name="P0" fontName="Helvetica" fontSize="11.0" leading="7" alignment="LEFT" spaceBefore="0.0" spaceAfter="4.0" textColor="#000000"/>
    <paraStyle name="P1" fontName="Helvetica" fontSize="10.0" leading="15" alignment="left" spaceBefore="0.0" spaceAfter="4.0" textColor="#000000"/>
    <paraStyle name="Program" fontName="Helvetica" fontSize="9.0" leading="15" alignment="left" spaceBefore="0.0" spaceAfter="4.0" textColor="#000000"/>
    <paraStyle name="P2" fontName="Helvetica" fontSize="4.0" leading="15" alignment="LEFT" spaceBefore="0.0" spaceAfter="4.0" textColor="#000000"/>
    <paraStyle name="P3" fontName="Helvetica" fontSize="8.0" leading="3" alignment="LEFT" spaceBefore="0.0" spaceAfter="4.0" textColor="#000000"/>
