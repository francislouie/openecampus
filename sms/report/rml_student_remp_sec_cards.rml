    <paraStyle name="space" fontName="Helvetica" fontSize="4.0" leading="5" alignment="LEFT" spaceBefore="0.0" spaceAfter="2.0" textColor="#000000"/>
    <paraStyle name="line" fontName="Helvetica" fontSize="15.0" leading="15" alignment="center" spaceBefore="0.0" spaceAfter="6.0" textColor="#000000"/>
    <paraStyle name="P5" fontName="Helvetica" fontSize="11.0" leading="15" alignment="CENTER" spaceBefore="3.0" spaceAfter="6.0" textColor="#000000"/>
    <paraStyle name="P6" fontName="Helvetica" fontSize="11.0" leading="15" alignment="CENTER" spaceBefore="0.0" spaceAfter="6.0" textColor="#000000"/>
