            id="report_class_transport_one_on_one_id"
            model="sms.transport.fee.payments"
            name="smstransport_print_one_student_per_page"
            multi="True"
            rml="smstransport/report/smstransport_print_one_student_per_page.rml"
            string="Transport Fee Receipts (Unpaid)"
            />  

		<!-- Report for printing transport challans in bulk from academic calendar Menu -->
		<report
            id="report_class_transport_three_on_one_id"
            model="sms.transport.fee.payments"
            name="smstransport_print_three_student_per_page"
            multi="True"
            rml="smstransport/report/smstransport_print_three_student_per_page.rml"
            string="Transport Fee Receipts (Unpaid)"
            />  

		<!-- Print students list in transport department from Transport Menu -->
