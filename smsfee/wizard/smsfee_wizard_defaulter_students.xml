<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

    <!-- Fee Reports -->

        <record id="view_defaulterfee_reports" model="ir.ui.view">
            <field name="name">fee.defaulters.form</field>
            <field name="model">fee.defaulters</field>
            <field name="arch" type="xml">
                <form string="Print Defaulter students Reports" version="7.0">
					<separator string="Session"/>
					<newline/>
					<field name="session" placeholder="Academic Session" nolabel="1" required="1"/>
                    <group>
						<newline/>
						<group>
	                      	<field name="class_id" placeholder="Classes" nolabel="1"/>
	                    </group>
						<newline/>
	                    <group>
							<field name="category" required="1"/>
							 <newline/><field name="order_by" required="1"/>
							 <newline/><field name="show_phone_no"/>
	                   </group>
					</group>
                    <footer>
                        <button string="Print Summary" name="print_defaulter_summary" type="object" class="oe_highlight"/>
                        <button string="Print Detailed Report" name="print_defaulter_detailed" type="object" class="oe_highlight"/>
                        or
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
               </form>
            </field>
          </record>

  <!-- action Fee Reports-->

        <record id="action_defaulterfee_reports" model="ir.actions.act_window">
            <field name="name">Defaulter Students</field>
            <field name="res_model">fee.defaulters</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
           <field name="view_id" ref="view_defaulterfee_reports"/>
           <field name="target">new</field>
        </record>
</data>
</openerp>